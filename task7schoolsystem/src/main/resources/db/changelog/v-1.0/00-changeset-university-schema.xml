            <sqlFile dbms="postgresql"
                     encoding="utf8"
                     endDelimiter="\nGO"
                     relativeToChangelogFile="true"
                     splitStatements="true"
                     stripComments="true"
                     path="00-drop-university-schema.sql"/>
        </rollback>
    </changeSet>
</databaseChangeLog>
